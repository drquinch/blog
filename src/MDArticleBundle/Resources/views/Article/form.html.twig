<div id="article_form">
    {{ form_start(form) }}

    {{ form_errors(form.coverimage) }}
    {{ form_widget(form.coverimage) }}<br>

    {{ form_errors(form.title) }}
    {{ form_widget(form.title, {'attr': {'id': 'input_title', 'value': 'Titre *'}}) }}<br>

    {{ form_errors(form.subtitle) }}
    {{ form_widget(form.subtitle, {'attr': {'id': 'input_subtitle', 'value': 'Sous-Titre'}}) }}<br>

    {{ form_errors(form.category) }}
    {{ form_widget(form.category) }}<br>

    {{ form_errors(form.content) }}
    {{ form_widget(form.content) }}<br>

    {{ form_label(form.published, "Publié") }}<br>
    {{ form_errors(form.published) }}
    {{ form_widget(form.published) }}<br>

    <span class="center">
    {{ form_label(form.games, "Jeux") }}<br>{{ form_errors(form.games) }}
    {{ form_widget(form.games, {'attr': {'id': 'input_game'}}) }}<a href="" class="addnew_button button">+ new game</a></span><br>

    <span class="center">
    {{ form_label(form.licences, "Licence") }}<br>{{ form_errors(form.licences) }}
    {{ form_widget(form.licences, {'attr': {'id': 'input_licence'}}) }}<a href="" class="addnew_button button">+ new licence</a></span><br>

    <span class="center">
    {{ form_label(form.publishers, "Éditeur") }}<br>{{ form_errors(form.publishers) }}
    {{ form_widget(form.publishers, {'attr': {'id': 'input_publisher'}}) }}<a href="" class="addnew_button button">+ new publisher</a></span><br>

    <span class="center">
    {{ form_label(form.developers, "Developpeur") }}<br>{{ form_errors(form.developers) }}
    {{ form_widget(form.developers, {'attr': {'id': 'input_developer'}}) }}<a href="" class="addnew_button button">+ new developer</a></span><br>

    <span class="center">
    {{ form_label(form.tags, "Tags") }}<br>{{ form_errors(form.tags) }}
    {{ form_widget(form.tags, {'attr': {'id': 'input_tags'}}) }}</span><br>

    <span class="center">{{ form_rest(form) }}
    {{ form_end(form) }}</span>
</div>
