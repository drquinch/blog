md_core_home:
    path:    /{batchHighlight}/{batch}/{page}
    defaults:
         _controller: MDCoreBundle:Home:home
         batch: 10
         page: 0
         batchHighlight: 3
    requirements:
         batch: \d*
         page: \d*
         batchHighlight: \d*

md_core_admin_home:
    path: /admin
    defaults:
        _controller: MDCoreBundle:Admin:home

md_core_contact:
    path:    /contact
    defaults: { _controller: MDCoreBundle:Contact:contact }

