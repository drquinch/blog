{% extends "MDCoreBundle::layoutadmin.html.twig" %}

{% block title %}administration des fiches de jeux{% endblock %}

{% block body %}

<p><a href="{{ path('md_core_game_game_create') }}" class="button_add_element">Ajouter une fiche de jeu</a></p>
<div id="list_elements">

<ul>
{% for l in listobjects %}
<li class="el_li"><a href="{{ path('md_core_game_game_view', {'id': l.id}) }}" class="el_name">{{ l.name }}</a> <a href="{{ path ('md_core_game_game_edit', {'id': l.id}) }}" class="el_edit">edit</a>
{% if is_granted('ROLE_ADMIN') %}<a href="{{ path('md_core_game_game_delete', {'id': l.id}) }}" class="el_delete">delete</a>{% endif %}
</li>
{% endfor %}
</ul>

<ul>
{% for page in paging %}
<li><a href="{{ path('md_core_admin_game_games', {'batch': batch, 'page': page}) }}">{{ page }}</a></li>
{% endfor %}
</ul>

</div>
{% endblock %}
